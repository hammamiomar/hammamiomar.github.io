---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---

<BaseLayout title = "Projects" sideBarActiveItemID="projects">
  <div>
    <div class="text-3xl w-full font-bold mb-5">Works In Progress...</div>
  </div>

  <HorizontalCard
    title="Diffusion Based Music Visualizer for Consumer Grade Devices"
    img="/projects/plantraits.webp"
    desc="A music visualizer which creates visualizations via stable diffusion XS and audio features via librosa."
    url="https://github.com/hammamiomar/DiffusionMusicVisualizer"
    badge="NEW"
  />
  
  <div>
    <div class="text-3xl w-full font-bold mb-5 mt-10">Older Projects</div>
  </div>
  <HorizontalCard
    title="PlantTraits2024 Challenge FGVC11"
    img="/projects/plantraits.webp"
    desc="A competition to predict plant traits from images and tabular data. My implementation uses image feature extractor and FiLM blocks...will make a blog post when im done. "
    url="https://github.com/hammamiomar/plantTraits/tree/main"
    badge="NEW"
  />
  <div class="divider my-0"></div>
  <HorizontalCard
    title="Youtube English To German Sub/Dub Conversion"
    img="/projects/german.webp"
    desc="Created a project with a Streamlit frontend that takes an English YouTube video and converts the English audio to German. Also provides German subtitles."
    url="https://youtu.be/2xcoopVLdYU"
    tags={["Whisper", "Text to Speech", "ffmpeg"]}
    date="Fall 2023"
  />
  <div class="divider my-0"></div>

  <HorizontalCard
    title="Data Visualization Generation Through Chat based Large Language Models"
    img="/projects/datavisllm.webp"
    desc="Project that generates code for visualizations that the user describes or uploads a picture of. Allows for fine-tuning of the visualization with more prompting."
    url="https://drive.google.com/file/d/1i26RnsPqoOrpf9kR0-dbRXOUJ5SsR4jK/view?usp=sharing"
    tags={["GPT-3", "Python"]}
    date="Spring 2023"
  />
  <div class="divider my-0"></div>

  <HorizontalCard
    title="Dataset Description Generation through Generated Knowledge Prompting with Large Language Models"
    img="/projects/datadesc.webp"
    desc="Implementing dataset description generation through contextual prompting."
